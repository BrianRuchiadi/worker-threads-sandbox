{
  "presets": [
    [
      "@babel/preset-env",
      {
        "exclude": [
          "@babel/plugin-transform-classes"
        ]
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-async-generators",
    "dynamic-import-node-babel-7",
    "@babel/plugin-syntax-top-level-await"
  ]
}